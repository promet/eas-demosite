{
    "minimum-stability": "alpha",
    "name": "drupal/prototype",
    "repositories": [
        {
            "type": "composer",
            "url": "http://static.drupal-packagist.org/v0.1.0"
        }
    ],
    "require": {
        "drupal/drupal": "7.*",
        "drupal/tangler": "~0.1.3",
        "drupal/admin_menu": "7.*",
        "drupal/context": "7.*",
        "drupal/ctools": "7.*",
        "drupal/date": "7.*",
        "drupal/diff": "7.*",
        "drupal/entity": "7.*",
        "drupal/entityform": "7.*",
        "drupal/features": "7.*",
        "drupal/feeds": "7.*",
        "drupal/job_scheduler": "7.*",
        "drupal/module_filter": "7.*",
        "drupal/omega": "7.4.*",
        "drupal/panels": "7.*",
        "drupal/rules": "7.*",
        "drupal/strongarm": "7.*",
        "drupal/views": "7.*",
        "drupal/wysiwyg": "7.*",
        "drush/drush": "6.*",
        "winmillwill/settings_compile": "~2.1"
    },
    "require-dev": {
        "drupal/devel": "7.*"
    },
    "scripts": {
        "post-install-cmd": [
            "Drupal\\Tangler\\ScriptHandler::postUpdate",
            "vendor/bin/settings_compile config.yml.dist www/sites/default/settings.php"
        ],
        "post-update-cmd": [
            "Drupal\\Tangler\\ScriptHandler::postUpdate"
        ]
    },
    "type": "project"
}
